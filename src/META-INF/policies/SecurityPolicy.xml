<wsp:Policy wsu:Id="SecurityServiceBindingPolicy"
	xmlns:wsu="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-utility-1.0.xsd"
	xmlns:wsp="http://schemas.xmlsoap.org/ws/2004/09/policy" xmlns:sp="http://docs.oasis-open.org/ws-sx/ws-securitypolicy/200702">
	<wsp:ExactlyOne>
		<wsp:All>
			<sp:SupportingTokens>
				<wsp:Policy>
					<sp:UsernameToken
						sp:IncludeToken="http://docs.oasis-open.org/ws-sx/ws-securitypolicy/200702/IncludeToken/AlwaysToRecipient" >
						<wsp:Policy>
							<wsp:All>
								<sp:WssUsernameToken11/>
							</wsp:All>
						</wsp:Policy>
					</sp:UsernameToken>
				</wsp:Policy>
			</sp:SupportingTokens>
			<sp:TransportBinding>
                 <wsp:Policy>
                    <sp:TransportToken>
                       <wsp:Policy>
                          <sp:HttpsToken
                             RequireClientCertificate="false" />
                       </wsp:Policy>
                    </sp:TransportToken>
                    <sp:AlgorithmSuite>
						<wsp:Policy>
							<sp:Basic256 />
							<sp:Basic192 />
							<sp:Basic128 />
						</wsp:Policy>
					</sp:AlgorithmSuite>
                 </wsp:Policy>
              </sp:TransportBinding>
              <sp:Layout>
			  	<wsp:Policy>
					<sp:Lax />
				</wsp:Policy>
			  </sp:Layout>
		</wsp:All>
	</wsp:ExactlyOne>
</wsp:Policy>